{
  "name": "node-memoization-example",
  "version": "1.0.0",
  "description": "example of memoization of api calls",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@types/prettier": "^1.18.3",
    "@types/redis": "^2.8.14",
    "@types/redis-mock": "^0.17.0",
    "@types/source-map-support": "^0.5.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "redis-mock": "^0.46.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist",
      "**/*.spec.(ts|tsx)"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
  }
